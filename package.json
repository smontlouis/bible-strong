{
  "name": "bible-strong",
  "version": "17.1.9",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android-staging": "react-native run-android --variant=stagingRelease --appIdSuffix=staging",
    "android-prod": "react-native run-android --variant=prodRelease",
    "ios": "react-native run-ios --simulator=\"iPhone 14\"",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "postversion": "react-native-version --is-bare-expo-workflow",
    "clean": "rm -rf yarn-lock.json && rm -rf node_modules && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && yarn install && say I love react native",
    "postinstall": "patch-package",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "storybook": "start-storybook -p 7007",
    "prestorybook": "rnstl",
    "nuke": "rm -rf node_modules; rm -rf ios/build; rm -rf ios/Pods; rm -rf ios/Podfile.lock; yarn; yarn podinstall",
    "i18n": "i18next 'src/**/*.{js,ts,tsx}' '!src/features/bible/bibleWebView/**' '!src/features/studies/studiesWebView/**'"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "modulePathIgnorePatterns": [
      ".yarn",
      ".npm"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Dev-iphonesimulator/Bible Strong - Dev.app",
        "build": "xcodebuild -workspace ios/biblestrong.xcworkspace -scheme 'biblestrong - dev' -configuration Dev -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11 Pro"
        }
      }
    },
    "test-runner": "jest"
  },
